.login_title {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.login_title .login_logo {
  width: 50%;
  height: 100rpx;
  margin: 20rpx auto;
}
.login_title .login_center {
  margin: 30rpx auto;
  padding: 80rpx 0;
  background: #0e6eb8;
}
.login_title .login_center .login_uesr {
  padding: 20rpx 170rpx;
  zoom: 1;
}
.login_title .login_center .login_uesr .login_uesr_p1 {
  color: white;
  font-size: 24rpx;
  float: left;
}
.login_title .login_center .login_uesr .login_uesr_p2 {
  color: white;
  font-size: 24rpx;
  float: right;
}
.login_title .login_center .login_uesr .login_uesr_img {
  float: left;
  width: 60rpx;
  height: 60rpx;
}
.login_title .login_center .login_uesr .login_phone_img {
  float: left;
  width: 60rpx;
  height: 60rpx;
}
.login_title .login_center .login_uesr .login_yzm_img {
  float: left;
  width: 60rpx;
  height: 60rpx;
}
.login_title .login_center .login_uesr .login_psw_img {
  float: left;
  width: 60rpx;
  height: 60rpx;
}
.login_title .login_center .login_uesr .login_uesr_input {
  background: rgba(255, 255, 255, 0.19);
  width: 330rpx;
  float: left;
  margin-left: 20rpx;
  height: 60rpx;
  border: none;
  color: white;
  text-indent: 1em;
  font-size: 24rpx;
}
.login_title .login_center .login_uesr .login_yzm_input {
  background: rgba(255, 255, 255, 0.19);
  width: 180rpx;
  float: left;
  margin-left: 20rpx;
  height: 60rpx;
  border: none;
  color: white;
  text-indent: 1em;
  font-size: 24rpx;
}
.login_title .login_center .login_uesr .login_psw_input {
  background: rgba(255, 255, 255, 0.19);
  width: 330rpx;
  float: left;
  margin-left: 20rpx;
  height: 60rpx;
  border: none;
  color: white;
  text-indent: 1em;
  font-size: 24rpx;
}
.login_title .login_center .login_uesr .login_uesr_btn {
  background: rgba(255, 255, 255, 0.19);
  color: white;
  font-size: 30rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border: none;
}
.login_title .login_center .login_uesr .login_yzm_btn {
  width: 140rpx;
  color: white;
  font-size: 24rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border: 1px solid rgba(255, 255, 255, 0.19);
  padding: 0 5rpx;
  margin-left: 10rpx;
  float: left;
}
.login_title .login_center .login_uesr::after {
  content: '';
  clear: both;
  display: block;
}

